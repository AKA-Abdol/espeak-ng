phoneme n
  IF nextPhW(isVelar) THEN // /n/ is velarized to /N/ when before velar stops
    ChangePhoneme(N)
  ENDIF
  CALL base1/n
endphoneme


phoneme aE
  vwl starttype #a endtype #e
  length 270
  FMT(vdiph/ae)
endphoneme

phoneme aI
  vwl starttype #a endtype #i
  length 240
  FMT(vdiph/ai)
endphoneme

phoneme ui
  vwl starttype #u endtype #i
  length 270
  lng
  FMT(vdiph/ui_4)
endphoneme
