phoneme n
  IF nextPhW(isVelar) THEN // /n/ is velarized to /N/ when before velar stops
    ChangePhoneme(N)
  ENDIF
  CALL base1/n
endphoneme


phoneme ui
  vwl starttype #u endtype #i
  length 270
  lng
  FMT(vdiph/ui_4)
endphoneme

phoneme iu
  vwl starttype #i endtype #u
  length 280
  lng
  FMT(vdiph2/iu_2)
endphoneme
